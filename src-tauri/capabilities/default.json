{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "opener:allow-open-path",
    "dialog:default",
    "notification:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "binaries/infer_worker",
          "sidecar": true
        }
      ]
    }
  ]
}